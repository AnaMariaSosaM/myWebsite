#gallery {
  margin 0
  padding 0
  list-style none
  
  & li {
    float left
    width 45%
    margin 2.5%
    background-color #f5f5f5
    color #bdc3c7
    
    & a p {
      margin 0
      padding 5%
      font-size 0.75em
      color #7c7b7c
      text-align center
    }
  }
  

}

.contact-info{
	list-style none
	padding 0
	margin 0
	font-size 0.9em
	
	& li.phone a {
	  background-image url('/images/phone.png')
	}

	& li.mail a {
	  background-image url('/images/mail.png')
	}

	& li.twitter a {
	  background-image url('/images/twitter.png')
	}
	
	& a {
		display block
		min-height 20px
		background-repeat no-repeat
		background-size 20px 20px
		padding 0 0 0 30px
		margin 0 0 10px
	}
}